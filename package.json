{
  "name": "mymonerepo",
  "version": "1.0.0",
  "description": "",
  "private": "true",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm --filter=@mymonerepo/* run build",
    "clear": "rimraf 'packages/*/{lib,node_modules}' && rimraf node_modules",
