  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.24.2",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0"
  }
}
